{
  "name": "vuepress-blog",
  "version": "1.0.0",
  "description": "基于vuepress搭建的博客",
  "scripts": {
    "start": "npm run dev1",
    "dev": "nodemon --watch docs bin/sidebar.js & ./node_modules/.bin/vuepress dev .",
    "dev1": "vuepress dev .",
    "build": "node bin/sidebar.js && node bin/changeMd.js && vuepress build .",
    "release": "gh-pages -d ./.vuepress/dist",
    "deploy": "npm run build && npm run release"
  },
  "author": "shimingw",
  "license": "MIT",
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.2.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "@vuepress/plugin-pwa": "^1.2.0",
    "ejs": "^3.0.1",
    "gh-pages": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2",
    "tracer": "^1.0.2",
    "vuepress": "^1.2.0",
    "vuepress-plugin-viewer": "^1.0.0",
    "yaml-front-matter": "^4.1.0"
  },
  "dependencies": {
    "vuepress-plugin-comment": "^0.7.2"
  }
}
